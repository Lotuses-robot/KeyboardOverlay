{
    "name": "Keyboard Overlay",
    "version": "3.0.0",
    "main": "main.js",
    "dependencies": {
        "app": "^0.1.0",
        "browser-window": "^0.4.0",
        "electron": "^12.0.0",
        "iohook": "^0.9.3"
    },
    "scripts": {
        "rebuildnew": "npm rebuild --runtime=electron --target=12.0.0 --disturl=https://atom.io/download/atom-shell --abi=87",
        "start": "electron ."
    },
    "iohook": {
        "targets": [
            "electron-87"
        ],
        "platforms": [
            "win32",
            "darwin",
            "linux"
        ],
        "arches": [
            "x64",
            "ia32"
        ]
    },
    "devDependencies": {
        "node-abi": "^3.15.0"
    }
}
